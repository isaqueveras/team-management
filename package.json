{
   "name": "team-menagement",
   "version": "0.0.1",
   "description": "backend para o sistema de gerenciamento de equipes",
   "author": "Isaque VÃ©ras",
   "scripts": {
      "start": "ts-node -r dotenv/config src/main/index.ts",
      "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main/index.ts",
      "clean": "rimraf dist coverage",
      "build": "npm run clean && tsc -p tsconfig-build.json",
      "prepare": "husky install .husky && cd .husky && sudo chmod +x pre-commit pre-push",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "check": "npm-check -s -u",
      "lint": "eslint .",
      "lint:fix": "npm run lint -- --fix",
      "test": "jest --passWithNoTests --no-cache --runInBand",
      "test:watch": "npm t -- --watch",
      "test:staged": "npm t -- --findRelatedTests",
      "test:coverage": "npm t -- --coverage",
      "test:integration": "npm t -- --config ./jest.integration.config.js"
   },
   "license": "ISC",
   "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.17.3",
      "jsonwebtoken": "^8.5.1",
      "module-alias": "^2.2.2",
      "pg": "^8.7.3",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "ts-node": "^10.8.1",
      "typeorm": "^0.2.29",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@jest-mock/express": "^1.4.5",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/jest": "^27.4.1",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^17.0.23",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.18.0",
      "dotenv": "^16.0.0",
      "eslint": "^8.12.0",
      "eslint-config-standard-with-typescript": "^21.0.1",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.0.0",
      "husky": "^7.0.4",
      "jest": "^27.5.1",
      "jest-mock-extended": "^2.0.5",
      "lint-staged": "^11.0.1",
      "mockdate": "^3.0.5",
      "pg-mem": "^2.3.5",
      "supertest": "^6.2.2",
      "ts-jest": "^27.1.4",
      "ts-node-dev": "^1.1.8",
      "typescript": "^4.6.3"
   },
   "keywords": [
      "ddd",
      "tdd",
      "clean architecture",
      "design patterns",
      "solid",
      "clean code",
      "node",
      "typescript",
      "typeorm",
      "jest"
   ],
   "engines": {
      "node": "16.x"
   }
}
